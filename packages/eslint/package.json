{
  "name": "@shared-config/eslint",
  "version": "1.1.1",
  "description": "All configs are here for making your development easier like EsLint, Prettier, TailWind, tsconfig etc.",
  "author": "Hridoy Ahmed",
  "license": "MIT",
  "repository": "https://github.com/codewithhridoy/config.git",
  "bugs": "https://github.com/codewithhridoy/config/issues",
  "homepage": "https://github.com/codewithhridoy/config#readme",
  "main": "index.js",
  "scripts": {
    "release": "pnpm run build && changeset publish",
    "publish-packages": "pnpm publish --access public --no-git-checks --provenance",
    "test:publish-packages": "pnpm publish --access public --no-git-checks --provenance --dry-run",
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "tailwind",
    "tailwind-config",
    "tsconfig",
    "typescript",
    "typescript-config",
    "shared-configs",
    "prettier",
    "prettier-config",
    "javascript",
    "javascript-config",
    "config",
    "style-guide",
    "turbo",
    "renovate",
    "renovate-config"
  ],
  "dependencies": {
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "5.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "packageManager": "pnpm@8.6.12"
}
